postgres:
  uri: "postgresql://{{ .Values.postgres.user }}:{{ .Values.postgres.password }}@{{ .Values.postgres.host }}:{{ .Values.postgres.port }}/{{ .Values.postgres.database }}{{ .Values.postgres.extraParamsUri }}"
  jdbcUri: "jdbc:postgresql://{{ .Values.postgres.host }}:{{ .Values.postgres.port }}/{{ .Values.postgres.database }}?user={{ .Values.postgres.user }}&password={{ .Values.postgres.password }}{{ .Values.postgres.extraParamsJdbcUri }}"
